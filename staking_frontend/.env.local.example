# Copy this file to `.env.local` in the staking_frontend folder and update values.
# Values prefixed with NEXT_PUBLIC_ are exposed to the browser by Next.js

# The staking contract principal (format: SP... .contract-name)
NEXT_PUBLIC_STAKING_CONTRACT_ADDRESS=SP3FBR2AGK5H9QBDH3EEN6DF8EK8JY7RX8QJ5SVTE.your-contract-name

# The staking token contract (used for prepareTransaction token param)
NEXT_PUBLIC_STAKING_TOKEN_CONTRACT=SP3FBR2AGK5H9QBDH3EEN6DF8EK8JY7RX8QJ5SVTE.your-token-contract

# Stacks network to use: "mainnet" or "testnet"
NEXT_PUBLIC_STACKS_NETWORK=mainnet
